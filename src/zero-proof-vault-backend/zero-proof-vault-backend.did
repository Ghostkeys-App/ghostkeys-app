type Column = record {
  id: text;
  name: text;
  hidden: bool;
};

type Row = record {
  id: text;
  values: vec text;
};

type VaultData = record {
  name: text;
  columns: vec Column;
  rows: vec Row;
};

type UserId = vec nat8;

service : {
  "add_or_update_vault": (UserId, VaultData) -> ();
  "get_my_vault": (UserId) -> (opt VaultData) query;
}
