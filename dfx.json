{
  "canisters": {
    "factory-canister-backend": {
      "candid": ".dfx/canisters/factory_canister_backend/factory-canister-backend.did",
      "type": "custom",
      "wasm": ".dfx/canisters/factory_canister_backend/factory_canister_backend.wasm",
      "metadata": [
        {
          "name": "candid:service"
        }
      ],
      "build": [
        "./scripts/fetch_shared_vault_release.sh",
        "./scripts/fetch_vault_release.sh",
        "./scripts/fetch_factory_release.sh",
        "./scripts/generate_declarations.sh"
      ]
    },
    "zero-proof-vault-frontend": {
      "dependencies": [
        "factory-canister-backend"
      ],
      "source": [
        "src/zero-proof-vault-frontend/dist"
      ],
      "type": "assets",
      "workspace": "zero-proof-vault-frontend"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "output_env_file": ".env",
  "version": 1
}